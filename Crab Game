int y1 = 0;
int x1 = 0;
int speed = 1;

void setup()
{
    size(800, 800);
    background(255, 255, 255);
}

void draw()
{
    Crab crab1 = new Crab (mouseX, mouseY);
}








class Crab
{
   int myX = 0;
   int myY = 0;
   //Constructor method for Bowl
   public Crab(int x, int y)
   {
      myX = x;
      myY = y; 
      ellipseMode(CORNER);
      ellipse(x, y, 100, 40);
   }
   
}
